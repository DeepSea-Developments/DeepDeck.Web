{
  "name": "esp-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-deepdeck": "ng build --prod --output-hashing none && npm run compress-artifacts && npm run move-artifacts",
    "compress-artifacts": "gzip-all \"dist/esp-frontend/*.{css,js}\" && rimraf dist/esp-frontend/*.js && rimraf dist/esp-frontend/*.css",
    "move-artifacts": "move .\\dist\\esp-frontend\\*.* .\\..\\DeepDeck.Ahuyama.fw\\spiffs_image",
    "bundle-analyze": "ng build --prod --stats-json --output-hashing none && webpack-bundle-analyzer dist/esp-frontend/stats.json",
    "serve-artifacts": "lite-server --baseDir=\"dist/esp-frontend\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "angular-animations": "^0.11.0",
    "bootstrap": "^4.6.0",
    "install": "^0.13.0",
    "moment": "^2.29.0",
    "ng-material-multilevel-menu": "^5.5.3",
    "ng2-date-picker": "^10.0.0",
    "ngx-moment": "^5.0.0",
    "ngx-socket-io": "^3.2.0",
    "npm": "^6.14.8",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.5",
    "codelyzer": "^6.0.0",
    "gzip-all": "^1.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.6"
  }
}